/* istanbul ignore next */'use strict';Object.defineProperty(exports, "__esModule", { value: true });exports.default = isWithinStep;function isWithinStep(value, step) {
  var decimals = String(step).split('.')[1];
  var corrector = typeof decimals !== 'undefined' && decimals.length > 0 ?
  Math.pow(10, decimals.length) :
  1;

  return value * corrector % (step * corrector) === 0;
} /* istanbul ignore next */module.exports = exports['default'];