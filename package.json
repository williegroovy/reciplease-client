{
  "name": "richardson-demos",
  "version": "1.0.0",
  "description": "Describe it here",
  "keywords": [
    "react",
    "redux",
    "demo"
  ],
  "main": "index.js",
  "homepage": "http://w-richardson.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/williegroovy/richardson-demos"
  },
  "scripts": {
    "start": "webpack-dev-server --inline --hot --history-api-fallback --host localhost --port 8080",
    "dev": "webpack --config webpack.dev.config.js",
    "prod": "webpack -p --config webpack.prod.config.js",
    "test": "mocha --recursive --compilers js:babel-register --require babel --require ignore-styles",
    "watch": "npm test -- --watch"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-1"
    ],
    "plugins": [
      [
        "import",
        {
          "libraryName": "antd",
          "style": "css"
        }
      ]
    ]
  },
  "author": "William Richardson",
  "license": "MIT",
  "dependencies": {
    "antd": "^2.12.0",
    "lodash": "^3.10.1",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-bootstrap": "^0.31.0",
    "react-bootstrap-table": "^3.3.8",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-transition-group": "^2.0.2",
    "redux": "^3.0.4",
    "redux-form": "^6.8.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "axios": "^0.16.2",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-plugin-import": "^1.2.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "6.5.0",
    "babel-runtime": "^6.23.0",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "ignore-styles": "^5.0.1",
    "mocha": "^3.4.2",
    "node-sass": "^4.5.3",
    "postcss-cssnext": "^3.0.0",
    "postcss-loader": "^2.0.6",
    "react-addons-test-utils": "^0.14.7",
    "react-hot-loader": "^1.3.1",
    "react-inline-transition-group": "^2.1.6",
    "react-svg": "^2.1.21",
    "recompose": "^0.23.5",
    "redux-immutable-state-invariant": "^2.0.0",
    "redux-saga": "^0.15.4",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.1",
    "watch": "^1.0.2",
    "webpack": "^1.14.0",
    "webpack-babel-link": "^1.1.0",
    "webpack-dev-server": "^1.14.0"
  }
}
